{
  "name": "waifu2x",
  "private": true,
  "version": "0.1.0",
  "main": "main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "electron .",
    "startall": "npm run dev && electron .",
    "exe-build": "vite build & electron-builder --config electron.config.json"
  },
  "build": {
    "productName": "waifu2x",
    "appId": "com.alc.pkg",
    "copyright": "@ 2022 alchemaniaC",
    "directories": {
      "output": "build"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./build/icons/aaa.ico",
      "uninstallerIcon": "./build/icons/bbb.ico",
      "installerHeaderIcon": "./build/icons/aaa.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "xxxx"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/aims.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@element-plus/icons-vue": "^2.0.10",
    "@paddlejs-models/ocr": "^1.2.3",
    "baidu-aip-sdk": "^4.16.7",
    "electron-store": "^8.1.0",
    "element-plus": "^2.2.28",
    "node-cmd": "^5.0.0",
    "path-browserify": "^1.0.1",
    "sagiri": "^3.2.4",
    "save": "^2.9.0",
    "tesseract.js": "^4.0.2",
    "vue": "^3.2.37",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/path-browserify": "^1.0.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "electron": "^21.2.0",
    "electron-builder": "^23.6.0",
    "electron-win-state": "^1.1.22",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^3.1.0"
  }
}
